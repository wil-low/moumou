    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) console",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/console/moumou",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/console",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "gbsim",
            "type": "cppdbg",
            "request": "launch",
            "program": "/home/willow/program/gbsim/build/gbsim",
            "args": [
                "moumou.ino.elf"
            ],
            "cwd": "${workspaceFolder}/gamebuino/build",
            "environment": [],
            "externalConsole": false
        }
    ]
}